<div class="wrapper d-flex align-items-stretch" ng-app="printlabels_app" ng-controller="Shipments_PrintLabels_Skus_controller">





    <nav id="sidebar">
        <div class="custom-menu">
            <button type="button" id="sidebarCollapse" class="btn btn-primary">
                <i class="fa fa-bars"></i>
                <span class="sr-only">Toggle Menu</span>
            </button>
        </div>
        <div class="p-4">
            <h1><a href="index.html" class="logo">Shipments <span>Print Labels</span></a></h1>

            <div class="mb-5">
                <div class="form-group has-search">
                    <span class="fa fa-search form-control-feedback"></span>
                    <input type="text" class="form-control" placeholder="Search" ng-model="search_shp">
                </div>
            </div>

            <section ng-show="loadingTvShipment">
                <div class="spinner-border" style="width: 3rem; height: 3rem;" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
                <div class="spinner-grow" style="width: 3rem; height: 3rem;" role="status">
                    <span class="sr-only">Loading...</span>
                </div>
            </section>


            <ul id="tvShipments" class="list-unstyled components mb-5" style="display:none;">
                <li ng-repeat="sp in stp_Shipments_GetWorkingShipments_Result |filter:search_shp" class="p"
                    id="cls_{{sp.ShipmentID}}" ng-click="Get_ShipmentSkus([sp.ShipmentID],[sp.ShipmentName], '')">

                    <div class="row">
                        <div class="col-md-6"><span><b>{{sp.ShipmentName.substr(0, 13)}}</b></span><br></div>
                        <div class="col-md-3"><span>{{sp.ShipmentID}}</span></div>
                        <div class="col-md-3"><span>{{sp.DestFulfmtCenterID}}</span></div>
                    </div>
                    <div class="row">
                        <div class="col-md-6"><span><b>{{sp.AmzShipmentID}}</b></span><br></div>
                        <div class="col-md-3">Pallets:<b>{{sp.CntPallets}}</b></div>
                        <div class="col-md-3">{{(100 * sp.PercentComplete) | number:2-1 }}%</div>
                    </div>
                    <div class="row">
                        <div class="col-md-12">
                            <div class="progress" style="width: {{(100 * sp.PercentComplete) }}%; height:5px;"></div>
                        </div>
                    </div>
                </li>
            </ul>


            <div class="footer">
                <p>
                    Copyright &copy;
                    <script>document.write(new Date().getFullYear());</script> All rights reserved | This template is made with <i class="icon-heart" aria-hidden="true"></i> by <a href="https://colorlib.com" target="_blank">Colorlib.com</a>
                </p>
            </div>
        </div>
    </nav>

