<script type="text/javascript">

    $(function () {


        var $callback = $("#callback");
        $("[multiple=multiple]")

            .filter(".single")


            .multiselect({
                multiple: false,
                noneSelectedText: 'Please select a radio',
                header: false
            })

            .end()
            .not(".single")
            .multiselect({

                checkAll: function () {

                    $callback.text("Check all clicked!");
                },
                close: function () {
                    alert("d");
                    $callback.text("Select closed!");
                }
            });
    });
</script>



